{
  "package": {
    "productName": "My Super App Name",
    "version":"../package.json"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://127.0.0.1:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": [
        "dmg",
        "deb",
        "appimage",
        "msi"
      ],
      "identifier": "com.example.tauri-template",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Â© 2022 tauri-template",
      "category": "DeveloperTool",
      "shortDescription": "simple inti off new app",
      "longDescription": "here you can take your time to explain all cool features your app do",
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "10.13",
        "providerShortName":"tauri-template",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "shell": {
        "open": true
      }
    },	
		"security": {
			"csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
		}
  }
}
